---
interface Skill {
  skills: Array<string>
}
const { title } = Astro.props
const { skills } = Astro.props as Skill
---
<div>
  <h3 class="text-slate-200 font-bold uppercase">{title}</h3>
  <ul>
    {
      skills.map(skill => <li class="capitalize text-slate-300 font-extralight">{skill}</li>)
    }
  </ul>
</div>
